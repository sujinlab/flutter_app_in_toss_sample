{
  "name": "@react-native-bedrock/utils",
  "type": "module",
  "version": "0.0.25",
  "description": "Utilities for Bedrock",
  "scripts": {
    "prepack": "yarn build",
    "typecheck": "tsc --noEmit",
    "test": "vitest --run",
    "build": "tsup"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/yauzl": "^2.10.3",
    "tsup": "^8.3.6",
    "typescript": "^5.6.3",
    "vitest": "^3.1.4"
  },
  "dependencies": {
    "yauzl": "^3.2.0"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "fbde03d4d9a1febc90124311f38f03bcc51a54a3"
}
